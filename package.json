{
  "devDependencies": {
    "cypress": "^13.4.0",
    "mochawesome": "^7.1.3"
  },
  "scripts": {
    "cy:run": "cypress run --reporter mochawesome",
    "mochawesome:merge": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "mochawesome:generate": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "test": "npm run cy:run && npm run mochawesome:merge && npm run mochawesome:generate"
  }
}
